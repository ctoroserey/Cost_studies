pages = {1},
publisher = {Springer US},
title = {{Foraging for foundations in decision neuroscience: insights from ethology}},
url = {http://www.nature.com/articles/s41583-018-0010-7},
year = {2018}
}
@article{Kool2018,
abstract = {Mental effort is an elementary notion in our folk psychology and a familiar fixture in everyday introspective experience. However, as an object of scientific study, mental effort has remained rather elusive. Cognitive psychology has provided some tools for understanding how effort impacts performance, by linking effort with cognitive control function. What has remained less clear are the principles that govern the allocation of mental effort. Under what circumstances do people choose to invest mental effort, and when do they decline to do so? And what regulates the intensity of mental effort when it is applied? In new and promising work, these questions are being approached with the tools of behavioural economics. Though still in its infancy, this economic approach to mental effort research has already uncovered important aspects of effort-based decision-making, and points clearly to future lines of inquiry, including some intriguing opportunities presented by recent artificial intelligence research.},
annote = {Mental effort expenditure is a cost-benefit trade-off. Animals/humans choose the least effortful of 2 equally long tasks, and will mainly work extra (including model-based strategizing) for significantly more reward (cog misers). It can also be rewarding and its value heterogeneous (learned industriousness).

No consensus on whether effort discounting is convex or concave. LST suggests concavity in the evaluation of salary and downtime for a given work.

Not due to metabolic depletion. Cog effort could be a heuristic to economize computational resources and avoid missing out on parallel objects of interest (OC, Musslick). AI sims suggest that cost of control and effort could help avoid task rule overfitting, giving way to more advantageous generalizable default principles that are more habitual and free up control.},
author = {Kool, Wouter and Botvinick, Matthew},
doi = {10.1038/s41562-018-0401-9},
file = {:Users/Claudio/Library/Application Support/Mendeley Desktop/Downloaded/Kool, Botvinick - 2018 - Mental labour.pdf:pdf},
issn = {23973374},
journal = {Nature Human Behaviour},
number = {December},
pages = {899--908},
publisher = {Springer US},
title = {{Mental labour}},
url = {http://dx.doi.org/10.1038/s41562-018-0401-9},
volume = {2},
year = {2018}
}
