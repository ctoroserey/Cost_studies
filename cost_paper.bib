keywords = {Cognitive control,Cognitive effort,Effort avoidance,Effort cost,Reward},
number = {June 2018},
pages = {41--54},
pmid = {29944865},
publisher = {Elsevier Ltd},
title = {{Neural systems of cognitive demand avoidance}},
