pmid = {24140077},
publisher = {Elsevier B.V.},
title = {{Parabolic discounting of monetary rewards by physical effort}},
url = {http://dx.doi.org/10.1016/j.beproc.2013.09.014},
volume = {100},
year = {2013}
}
